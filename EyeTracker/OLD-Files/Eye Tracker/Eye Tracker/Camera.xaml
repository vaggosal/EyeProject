<Window x:Class="Eye_Tracker.Camera"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Camera" Height="700" Width="700" Loaded="Window_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="199*"/>
            <ColumnDefinition Width="65*"/>
        </Grid.ColumnDefinitions>
        <Image x:Name="cameraImg" HorizontalAlignment="Left" Height="600" Margin="39,22,0,0" VerticalAlignment="Top" Width="600" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
