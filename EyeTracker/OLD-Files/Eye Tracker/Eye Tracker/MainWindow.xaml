        Title="MainWindow" Height="700" Width="700">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="171*"/>
            <ColumnDefinition Width="2*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="trackBtn" Content="Track" HorizontalAlignment="Left" Margin="59,399,0,0" VerticalAlignment="Top" Width="75" Click="trackBtn_Click"/>
        <Button x:Name="browseBtn" Content="Browse" HorizontalAlignment="Left" Margin="280,399,0,0" VerticalAlignment="Top" Width="75" Click="browseBtn_Click"/>
        <Button x:Name="ledBtn" Content="Led" HorizontalAlignment="Left" Margin="584,399,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
